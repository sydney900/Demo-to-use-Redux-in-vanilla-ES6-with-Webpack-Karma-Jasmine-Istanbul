{
  "name": "es6-redux-karma-jasmine-webpack-istanbul",
  "version": "0.1.0",
  "description": "Boilerplate for ES6 client-based library. Uses Babel and webpack for transpiling and building, Karma and Jasmine for unit testing, and Istanbul for code coverage.",
  "main": "src/redux/indexmyapp.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcroitoru/es6-karma-jasmine-webpack-istanbul-boilerplate.git"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "webpack --env development",
    "build:prod": "webpack -p --env production -d",
    "test": "karma start",
    "coverage": "karma start --single-run --no-auto-watch --coverage=true || echo done"
  },
  "keywords": [
    "es6",
    "redux",
    "babel",
    "webpack",
    "jasmine",
    "karma",
    "istanbul"
  ],
  "author": "Bing Wang",
  "license": "MIT",
  "dependencies": {
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.0",
    "css-loader": "^0.28.7",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.4.1"
  }
}